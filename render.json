{
  "name": "ancient-ocr",
  "type": "web",
  "env": "python",
  "buildCommand": "pip install -r requirements.txt",
  "startCommand": "gunicorn --bind 0.0.0.0:$PORT --workers 2 --timeout 300 app:app",
  "repo": "https://github.com/kimi-ai/ancient-ocr",
  "branch": "main",
  "envVars": [
    {
      "key": "PYTHON_VERSION",
      "value": "3.11.0"
    },
    {
      "key": "PORT",
      "value": "5000"
    }
  ],
  "disk": {
    "name": "uploads",
    "mountPath": "/app/uploads",
    "sizeGB": 2
  },
  "healthCheckPath": "/",
  "autoDeploy": true
}